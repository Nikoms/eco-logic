{
  "name": "eco-logic",
  "version": "1.0.0",
  "description": "Backend : ```  cd backend yarn start ```",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "cd src/frontend/vuejs && vue-cli-service build",
    "build-react": "cd frontend/reactts && tsc --build && parcel build src/index.html",
    "build-server": "tsc -p ./src/backend",
    "webpack": "webpack",
    "backend:dev": "NODE_ENV=development webpack --watch --config src/backend/webpack.backend.config.js",
    "backend:run:dev": "NODE_ENV=development nodemon dist/index.js --watch dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nikoms/eco-logic.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Nikoms/eco-logic/issues"
  },
  "homepage": "https://github.com/Nikoms/eco-logic#readme",
  "private": true,
  "workspaces": [
    "src/eco/*",
    "src/infrastructure",
    "src/frontend/infrastructure",
    "src/frontend/interface-adapter",
    "src/frontend/vuejs",
    "src/backend"
  ],
  "devDependencies": {
    "@types/graphql": "^14.2.3",
    "@types/node": "^12.6.8",
    "@types/restify": "^7.2.12",
    "@types/typescript": "^2.0.0",
    "@types/uuid": "^3.4.5",
    "ts-loader": "^6.0.4",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.14.0",
    "type-graphql": "^0.17.4",
    "typescript": "^3.3.4000",
    "webpack-cli": "^3.3.6",
    "webpack-node-externals": "^1.7.2",
    "webpack": "^4.38.0"
  },
  "dependencies": {
    "apollo-server-restify": "^1.3.6",
    "event-target-shim": "^5.0.1",
    "graphql": "^14.4.2",
    "nodemon": "^1.19.1",
    "reflect-metadata": "^0.1.13",
    "restify": "^8.4.0",
    "uuid": "^3.3.2"
  }
}
